[rpi]
192.1.1.10 #Adres IP Serwera na którym chcemy wykonać skrypty Ansible'a

[rpi:vars]
ansible_ssh_user=remote_server_username #nazwa użytkownika na serwerze zdalnym uprawnionego do wykonywania zmian
ansible_ssh_private_key_file=/home/actualUser/.ssh/ubuntuServer #ścieżka do prywatnego klucza SSH